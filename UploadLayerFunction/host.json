{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxDequeueCount": 1
    },
    "durableTask": {
      "storageProvider": {
        "maxQueuePollingInterval": "00:02:00",
        "partitionCount": 1
      },
      "storeInputsInOrchestrationHistory": true,
      "maxConcurrentActivityFunctions": 4,
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 30
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Exception"
      }
    }
  }
}